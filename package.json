{
  "name": "wrange",
  "version": "0.0.1",
  "main": "src/exports.js",
  "scripts": {
    "test": "npm run build && jest --verbose",
    "clean": "bsb -clean-world",
    "build": "bsb -make-world",
    "prepare": "npm run build",
    "start": "npm run build && node src/server.bs.js",
    "watch:test": "jest --watchAll",
    "watch:build": "bsb -make-world -w",
    "watch:start": "nodemon -e .bs.js --watch src src/server.bs.js"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "ELLIOTTCABLE <npm@ell.io>",
  "license": "ISC",
  "dependencies": {
    "@elliottcable/bs-cmdliner": "^1.0.2",
    "bs-express": "^0.8.0",
    "es6-symbol": "^3.1.1"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.2",
    "bs-platform": "^4.0.1",
    "nodemon": "^1.18.3",
    "ocaml-language-server": "^1.0.35"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.{js,jsx}",
      "!**/parser.*",
      "!**/server.*",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  }
}
