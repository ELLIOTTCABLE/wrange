{
  "name": "wrange",
  "version": "0.0.1",
  "main": "src/exports.js",
  "bin": "./src/cli.bs.js",
  "scripts": {
    "test": "npm run build && jest --verbose && bats __tests__",
    "clean": "bsb -clean-world",
