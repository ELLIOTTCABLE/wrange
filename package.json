    "es6-symbol": "^3.1.1",
    "type-is": "^1.6.18"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.10",
    "bats": "^1.1.0",
    "bats-assert": "^2.0.0",
    "bats-support": "^0.3.0",
    "bs-platform": "^4.0.1",
    "ocaml-language-server": "^1.0.35"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.{js,jsx}",
      "!**/parser.*",
      "!**/server.*",
