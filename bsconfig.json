{
  "name": "wrange",
  "version": "0.0.1",
  "sources": [
    {"dir": "src", "subdirs": true},
    {"dir": "__tests__", "type": "dev"}
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "bs-express"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest"
  ],
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header",
    "-bs-cross-module-opt"
  ],
  "warnings": {
    "error" : "+101"
  },
  "refmt": 3
}
